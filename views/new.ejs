<% include ./partials/header%>
<div id="container">
    <h1 style="text-align: center">Enter a new campground</h1>
    <div id="new-camp">
        <form action="/campgrounds" method="post" >
        <div class="form-group">
            <input class="form-control" type="text" name="name" placeholder="name">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="image" placeholder="image link">
        </div>
        <div class="form-group">
            <button class="btn btn-primary btn-block">Submit</button>
        </div>
        </form>
        <a href="/campgrounds"><button class="btn btn-danger btn-block">Cancel</button></a>
    </div>
</div>

<% include ./partials/footer%>